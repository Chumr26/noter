{
  "expo": {
    "name": "Noter",
    "slug": "noter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-dark.png",
    "scheme": "notesapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6366F1"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.notesapp.mobile",
      "jsEngine": "hermes"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1F2937",
        "foregroundImage": "./assets/images/icon-dark.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.notesapp.mobile",
      "jsEngine": "hermes",
      "allowBackup": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon-dark.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#6366F1",
          "dark": {
            "backgroundColor": "#1F2937",
            "image": "./assets/images/splash-icon.png"
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b7ec1ac6-acb1-4087-909a-8763f6902576"
      }
    }
  }
}
