{
    "cli": {
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "android": {
                "buildType": "apk"
            },
            "distribution": "internal"
        },
        "production": {
            "android": {
                "buildType": "apk",
                "gradleCommand": ":app:assembleRelease"
            },
            "ios": {
                "buildConfiguration": "Release"
            },
            "cache": {
                "key": "production-v1",
                "paths": ["node_modules"]
            }
        }
    },
    "submit": {
        "production": {}
    }
}